{
  "expo": {
    "name": "Naama Bloom",
    "slug": "naama-bloom-barbershop",
    "version": "1.0.1",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a25844b7-40a6-4320-98e3-878ea7f072e4"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nbeyebrows",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.naama.bloom",
      "buildNumber": "2",
      "deploymentTarget": "13.0",
      "googleServicesFile": "./app/GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "האפליקציה צריכה גישה למצלמה כדי לצלם תמונות פרופיל ולהעלות תמונות לגלריית המספרה",
        "NSPhotoLibraryUsageDescription": "האפליקציה צריכה גישה לגלריה כדי לבחור תמונות פרופיל ולהעלות תמונות למספרה",
        "NSContactsUsageDescription": "האפליקציה צריכה גישה לאנשי הקשר כדי לאפשר שיתוף פרטי המספרה בקלות",
        "NSLocationWhenInUseUsageDescription": "האפליקציה צריכה גישה למיקום כדי להראות לך כיוונים למספרה ולספק שירות טוב יותר",
        "NSUserNotificationsUsageDescription": "האפליקציה צריכה גישה להתראות כדי לעדכן אותך על התורים שלך ולשלוח התראות חשובות מהמספרה",
        "NSCalendarsUsageDescription": "האפליקציה צריכה גישה ליומן כדי להוסיף ולנהל את התורים שלך.",
        "LSApplicationQueriesSchemes": [
          "waze",
          "whatsapp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2,
      "package": "com.naama.bloom",
      "googleServicesFile": "./app/google-services.json",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "modularHeaders": true,
            "ccacheEnabled": true,
            "deploymentTarget": "13.0",
            "newArchEnabled": false,
            "swiftVersion": "5.0"
          },
          "android": {
            "targetSdkVersion": 34,
            "compileSdkVersion": 34,
            "enableLargeHeap": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a25844b7-40a6-4320-98e3-878ea7f072e4"
      }
    },
    "owner": "orel895"
  }
}

    "debug-android": "node ./scripts/debug-android-build.js",
    "build:android-dev": "eas build --platform android --profile development",
    "build:android-prod": "eas build --platform android --profile production",
    "update:dev": "eas update --branch dev --message \"dev update\"",
    "update:beta": "eas update --branch beta --message \"beta update\"",
    "update:prod": "eas update --branch production --message \"prod update\"",
    "postinstall": "patch-package",
    "prebuild:android": "npx patch-package --error-on-fail",
    "seed": "ts-node scripts/seed.ts"
